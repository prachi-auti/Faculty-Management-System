<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         version="3.1">
  <servlet>
    <servlet-name>AddFacultyServlet</servlet-name>
    <servlet-class>com.avcoe.controller.AddFacultyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddFacultyServlet</servlet-name>
    <url-pattern>/AddFacultyServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteFacultyServlet</servlet-name>
    <servlet-class>com.avcoe.controller.DeleteFacultyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteFacultyServlet</servlet-name>
    <url-pattern>/DeleteFacultyServlet</url-pattern>
  </servlet-mapping>
</web-app>
